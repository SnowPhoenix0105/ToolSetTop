    "paths": {
      "/encoding/baseinfo": {
        "post": {
          "summary": "设定基础信息",
          "description": "",
          "tags": [
            "编码"
          ],
          "parameters": [],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "target": {
                      "type": "string",
                      "title": "目标编码",
                      "enum": [
                        "utf8",
                        "bgk"
                      ],
                      "x-apifox": {
                        "enumDescriptions": {
                          "utf8": "UTF-8编码",
                          "bgk": "GBK编码"
                        }
                      }
                    },
                    "selected": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "title": "需要进行转码的文件"
                    },
                    "unselected": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "title": "不需要转码的文件"
                    },
                    "charset": {
                      "type": "array",
                      "items": {
                        "type": "integer"
                      },
                      "title": "字符集"
                    }
                  },
                  "required": [
                    "target",
                    "selected",
                    "unselected",
                    "charset"
                  ]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "成功",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string",
                          "title": "文件名"
                        },
                        "encoding": {
                          "type": "string",
                          "title": "源编码格式",
                          "enum": [
                            "utf8",
                            "gbk"
                          ],
                          "x-apifox": {
                            "enumDescriptions": {
                              "utf8": "UTF-8编码",
                              "gbk": "GBK编码"
                            }
                          }
                        }
                      },
                      "required": [
                        "name",
                        "encoding"
                      ]
                    }
                  },
                  "examples": {}
                }
              }
            }
          }
        }
      },
